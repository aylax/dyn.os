#!/bin/bash
docker stop $(docker ps -aq) && docker rm $(docker ps -aq)
docker rmi $(docker images | grep "none" | awk '{print $3}')
docker build -t aylax:lynn-basic -f docker.d/basic .
docker build -t aylax:lynn-dynls -f docker.d/dynls .
docker build -t aylax:lynn-final -f docker.d/final .
docker run -d -p 127.0.0.1:6000:6000/udp --name lynn aylax:lynn-final

